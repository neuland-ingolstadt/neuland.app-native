{
    "extends": "expo/tsconfig.base",
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@/*": ["./src/*"],
            "ios/*": ["./ios/NeulandNext/Images.xcassets/*"]
        },
        "project": "./tsconfig.json",
        "strict": true
    },
    "include": [
        "**/*.ts",
        "**/*.tsx",
        ".expo/types/**/*.ts",
        "expo-env.d.ts",
        "babel.config.js",
        "src/app/(screens)/meal.s"
    ]
}
