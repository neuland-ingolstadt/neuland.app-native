{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"baseUrl": "./",
		"paths": {
			"@/*": ["./src/*"],
			"ios/*": ["./ios/NeulandNext/Images.xcassets/*"]
		},
		"project": "./tsconfig.json",
		"strict": true,
		"plugins": [
			{
				"name": "@0no-co/graphqlsp",
				"schema": "src/__generated__/schema.graphql"
			}
		]
	},
	"include": [
		"**/*.ts",
		"**/*.tsx",
		".expo/types/**/*.ts",
		"expo-env.d.ts",
		"babel.config.js",
		"src/app/(screens)/meal.s",
		"src/app/(tabs)/(index)/links.tsx"
	]
}
