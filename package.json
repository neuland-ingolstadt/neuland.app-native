    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "prettier": "3.3.3",
    "prop-types": "^15.8.1",
    "typescript": "^5.7.2"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,json,yml, md}": [
      "bunx prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "bunx eslint --fix"
    ]
  },
  "patchedDependencies": {
    "@th3rdwave/react-navigation-bottom-sheet@0.3.2": "patches/@th3rdwave%2Freact-navigation-bottom-sheet@0.3.2.patch",
    "react-native-week-view@0.30.0": "patches/react-native-week-view@0.30.0.patch",
    "react-native-paper@5.12.5": "patches/react-native-paper@5.12.5.patch",
    "@aptabase/react-native@0.3.10": "patches/@aptabase%2Freact-native@0.3.10.patch"
  }
}
